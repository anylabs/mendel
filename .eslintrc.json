{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["typescript", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "parser": "typescript",
        "semi": false,
        "trailingComma": "all",
        "printWidth": 120
      }
    ]
  }
}
